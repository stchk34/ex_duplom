{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 8 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "waza-ari/ms-dropdown",
                "version": "3.5.2",
                "dist": {
                    "url": "https://github.com/waza-ari/msdropdown/archive/v3.5.2.zip",
                    "type": "zip"
                },
                "type": "drupal-library"
            }
        },
        {
            "type": "package",
            "package": {
                "name": "dinbror/blazy",
                "version": "1.8.2",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/dinbror/blazy/archive/refs/tags/1.8.2.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "kenwheeler/slick",
                "version": "1.8.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/kenwheeler/slick/archive/refs/tags/1.8.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "mreq/slick-lightbox",
                "version": "0.2.12",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/mreq/slick-lightbox/archive/refs/tags/v0.2.12.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "bower-asset/photoswipe": "^4",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "dinbror/blazy": "^1.8",
        "drupal/admin_toolbar": "^3.1",
        "drupal/advpoll": "^1.2",
        "drupal/better_exposed_filters": "^5.2",
        "drupal/better_login_form_config": "^2.4",
        "drupal/better_social_sharing_buttons": "^4.0",
        "drupal/blazy": "^2.5",
        "drupal/blazy_photoswipe": "^1.3",
        "drupal/block_access": "^1.0",
        "drupal/bootstrap_barrio": "^5.5",
        "drupal/colorbox": "^1.10",
        "drupal/content_moderation_notifications": "^3.4",
        "drupal/core-composer-scaffold": "^9",
        "drupal/core-project-message": "^9",
        "drupal/core-recommended": "^9",
        "drupal/ctools": "^3.10",
        "drupal/customer_identity_and_access_management": "^5.1",
        "drupal/devel": "^4.1",
        "drupal/dropdown_language": "^3.0",
        "drupal/entity": "^1.3",
        "drupal/entity_print": "^2.6",
        "drupal/entityqueue": "^1.2",
        "drupal/fancy_login": "^3.0",
        "drupal/faqfield": "^7.0",
        "drupal/field_limiter": "^1.1",
        "drupal/field_permissions": "^1.2",
        "drupal/field_slideshow": "^3.0@beta",
        "drupal/file_download_link": "^2.0",
        "drupal/flag": "^4.0@beta",
        "drupal/fontawesome": "^2.23",
        "drupal/google_analytics": "^4.0",
        "drupal/image_link_formatter": "^2.1",
        "drupal/lang_dropdown": "^2.0",
        "drupal/languageicons": "^1.0@beta",
        "drupal/league_oauth_login": "^1.5",
        "drupal/libraries": "^3.0@beta",
        "drupal/login_popup": "^2.0",
        "drupal/mailsystem": "^4.4",
        "drupal/menu_block": "^1.8",
        "drupal/menu_breadcrumb": "^1.16",
        "drupal/menu_position": "^1.0@beta",
        "drupal/mimemail": "^1.0@alpha",
        "drupal/miniorange_oauth_client": "^3.41",
        "drupal/notificationswidget": "^1.4",
        "drupal/oauth2_client": "^3.0",
        "drupal/openid_connect": "^1.2",
        "drupal/pathauto": "^1.10",
        "drupal/photoswipe": "^3.2",
        "drupal/poll": "^1.5",
        "drupal/registration_role": "^1.1",
        "drupal/rules": "^3.0@alpha",
        "drupal/rules_flag": "^1.0",
        "drupal/rules_token": "^2.0",
        "drupal/rules_webform": "^3.0",
        "drupal/scrollup": "^3.0",
        "drupal/search_api": "^1.25",
        "drupal/search_api_solr": "^4.2",
        "drupal/simple_gmap": "^3.0",
        "drupal/site_alert": "^1.2",
        "drupal/sitemap": "^2.0@beta",
        "drupal/sitewide_alert": "^2.0",
        "drupal/slick": "^2.6",
        "drupal/slick_lightbox": "^1.5",
        "drupal/slick_views": "^2.6",
        "drupal/smtp": "^1.0",
        "drupal/social_auth_facebook": "^3.0",
        "drupal/social_auth_google": "^3.0",
        "drupal/social_auth_linkedin": "^3.1",
        "drupal/social_auth_twitter": "^3.0",
        "drupal/super_login": "^2.0",
        "drupal/taxonomy_menu": "^3.5",
        "drupal/telephone_validation": "^2.3",
        "drupal/total_control": "^3.0",
        "drupal/twig_tweak": "^3.1",
        "drupal/views_field_view": "^1.0@beta",
        "drupal/visitors": "^2.9",
        "drupal/webform": "^6.1",
        "drupal/webform_query": "^1.0@alpha",
        "drupal/webform_views": "^5.0@beta",
        "drupal/webform_workflows_element": "^1.0@alpha",
        "drush/drush": "^10.2",
        "kenwheeler/slick": "^1.8",
        "mikehaertl/phpwkhtmltopdf": "~2.1",
        "mreq/slick-lightbox": "^0.2.12",
        "tecnickcom/tcpdf": "~6",
        "waza-ari/ms-dropdown": "^3.5"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "robloach/component-installer": false,
            "cweagans/composer-patches": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "patches": {
            "drupal/menu_block": {
                "Parent link": "https://www.drupal.org/files/issues/2020-10-12/menu_block-render-parent-3082445-26.patch"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
